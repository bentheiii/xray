fn main()->bool{
    assert(fraction(1,3).abs() == fraction(1,3))
    && assert(fraction(-1,3).abs() == fraction(1,3))
    && assert(fraction(1,-3).abs() == fraction(1,3))
    && assert(fraction(-1,-3).abs() == fraction(1,3))
    && assert(fraction(0,3).abs() == fraction(0,3))
    && assert(fraction(0,-3).abs() == fraction(0,-3))
}