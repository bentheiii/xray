fn range(start: int, stop: int, step:int)->Sequence<int> {
    fn range_helper(ret: Stack<int>, i:int)->Stack<int> {
        (i >= stop).if(
            ret,
            range_helper(ret.push(i), i + step)
        )
    }
    range_helper(stack(), start).to_array()
}


fn main()->bool {
    let a = range(1,20,3);
    assert(a == [1,4,7,10,13,16,19])
}